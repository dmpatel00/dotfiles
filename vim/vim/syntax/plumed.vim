" Vim syntax file
" Language: PLUMED

if exists("b:current_syntax")
  finish
endif

syntax case ignore

let b:current_syntax="plumed"

if exists("g:plumed_shortcuts")
  noremap  <buffer> <F2> :PHelp<CR>
  inoremap <buffer> <F2> <Esc>:PHelp<CR>
endif

